@model SnackListViewModel

<div>
    <h2>Todos os Lanches</h2>
    <h5>@ViewData["Date"]</h5>
</div>

<hr />
<h6>@ViewBag.TotalText @ViewBag.TotalSnacks</h6>
<hr />

<div class="container">
    <div class="row">
        @foreach (Snack snack in Model.Snacks) {
            <partial name="_SnacksResume" for="@snack"/>
        }
    </div>
</div>
